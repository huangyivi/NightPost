/* 顶部样式 */
/* 1px = 2rpx */
@import "../../../app.wxss";

page {
  background-color: #F8F8F8;
  font-family: 'KaiTi' !important;
}

.navi-top {
  background-color: #F8F8F8;
  top: 0;
  box-sizing: border-box;
  padding-left: 64rpx;
  padding-right: 64rpx;
  position: fixed;
  width: 100vw;
  font-size: 44rpx;
  font-weight: 500;
  line-height: 62rpx;
  color: #181818;
  opacity: 1;
}


.main {

  box-sizing: border-box;
  height: auto;
  width: 100vw;
}

.main-content {
  width: 100%;
  height: 100%;
}

.published-text {
  position: relative;
  top: -50rpx;
  width: 100%;
  height: 300rpx;
  font-size: 44rpx;
  font-family: KaiTi;
  font-weight: 500;
  line-height: 62rpx;
  color: #181818;
  text-shadow: 0rpx 0rpx 50rpx #000000;
  opacity: 1;
  background: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 100% auto;
  background-position-y: 50%;
}

.surprise {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 20rpx 32rpx 0;
  background: linear-gradient(#fff 80%, transparent);
  opacity: 0;
}

.surprise-title {
  font-size: 34rpx;
  font-weight: 500;
  line-height: 50rpx;
  color: #ACACAC;
  opacity: 1;
}

.surprise-ctx {
  font-size: 30rpx;
  font-family: KaiTi;
  font-weight: 500;
  line-height: 42rpx;
  color: #7B7B7B;
  opacity: 1;
}

.tip {
  margin:0 0 50rpx 0;
  width: 100%;
  font-size: 28rpx;
  font-family: KaiTi;
  font-weight: 400;
  line-height: 40rpx;
  color: #D2D2D2;
  opacity: 1;
}


.navi-bottom {
  background-color: #F8F8F8;
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  padding: 0 64rpx;
  height: 240rpx;
  width: 100vw;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.navi-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(0, 0, 0, 0.3);
  font-size: 30rpx;
  font-weight: 500;
  line-height: 42rpx;
  width: 56rpx;
  height: 56rpx;
}

.navi-btn-active {
  width: 112rpx;
  height: 112rpx;
  background-color: rgba(0, 0, 0, 0.1);
}